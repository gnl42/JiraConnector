<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="com.atlassian.connector.eclipse.subclipse"
      label="%featureName"
      version="2.0.0.qualifier"
      provider-name="%providerName"
      plugin="com.atlassian.connector.eclipse.ui">

   <description url="http://atlassian.com">
      %description
   </description>

   <copyright>
      %copyright
   </copyright>

   <license url="license.html">
      %license
   </license>

   <url>
      <update label="%updateSiteName" url="http://update.atlassian.com/atlassian-eclipse-plugin"/>
   </url>

   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="com.atlassian.connector.eclipse.ui"/>
      <import plugin="com.atlassian.connector.eclipse.team.ui"/>
      <import plugin="org.eclipse.mylyn.commons.core"/>
      <import plugin="org.eclipse.team.core"/>
      <import plugin="org.eclipse.core.filesystem"/>
      <import plugin="org.eclipse.ui.ide"/>
      <import plugin="org.eclipse.core.resources"/>
      <import feature="com.atlassian.connector.eclipse.crucible" version="2.0.0.qualifier" match="equivalent"/>
      <import plugin="org.tigris.subversion.subclipse.core" version="1.6.0" match="compatible"/>
      <import plugin="org.tigris.subversion.subclipse.ui" version="1.6.0" match="compatible"/>
      <import feature="org.tigris.subversion.subclipse" version="1.6.0" match="compatible"/>
      <import feature="org.tigris.subversion.clientadapter.feature" version="1.6.0" match="compatible"/>
   </requires>

   <plugin
         id="com.atlassian.connector.eclipse.subclipse.ui"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

	<plugin
         id="com.atlassian.connector.eclipse.subclipse.core"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
