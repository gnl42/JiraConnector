<?xml version="1.0" encoding="UTF-8"?>
<project name="org.eclipse.mylyn.jira.core" default="clean-source">
    <description>
            Generate SOAP stubs
    </description>

	<target name="clean-source">
		<replace dir="src/org/eclipse/mylyn/internal/jira/core/wsdl">
			<include name="**/*.java"/>
  			<replacetoken><![CDATA[
public ]]></replacetoken>
			<replacevalue><![CDATA[
@SuppressWarnings("all")
public ]]></replacevalue>
		</replace>
		<replace dir="src/org/eclipse/mylyn/internal/jira/core/wsdl">
			<include name="**/*.java"/>
  			<replacetoken><![CDATA[by the Apache Axis 1.4 Apr 22, 2006 (06:55:48 PDT) WSDL2Java emitter.]]></replacetoken>
			<replacevalue><![CDATA[by the Apache Axis 1.3 Oct 05, 2005 (05:23:37 EDT) WSDL2Java emitter.]]></replacevalue>
		</replace>
