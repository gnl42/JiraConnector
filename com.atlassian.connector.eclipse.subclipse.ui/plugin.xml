<plugin>
	<extension point="com.atlassian.connector.eclipse.ui.teamConnector">
		<teamConnector
			class="com.atlassian.connector.eclipse.internal.subclipse.ui.SubclipseTeamResourceConnector">
		</teamConnector>
	</extension>
	<extension point="org.eclipse.core.runtime.adapters">
		<factory adaptableType="org.tigris.subversion.subclipse.core.history.ILogEntry"
			class="com.atlassian.connector.eclipse.internal.subclipse.ui.LogEntryAdapterFactory">
			<adapter
				type="com.atlassian.connector.eclipse.ui.team.ICustomChangesetLogEntry" />
		</factory>
	</extension>

</plugin>
